        "schemas": {
            "ScimCreateUserDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "userName": {
                        "type": "string"
                    },
                    "name": {
                        "type": "object",
                        "properties": {
                            "givenName": {
                                "type": "string"
                            },
                            "middleName": {
                                "type": "string"
                            },
                            "familyName": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "givenName",
                            "familyName"
                        ]
                    },
                    "emails": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "primary": {
                                    "type": "boolean"
                                },
                                "value": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "value"
                            ]
                        }
                    },
                    "active": {
                        "type": "boolean"
                    },
                    "statsigOrgRole": {
                        "type": "string",
                        "nullable": true
                    },
                    "schemas": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "userName",
                    "name",
                    "emails"
                ]
            },
            "ScimPartialUpdateUserDto": {
                "type": "object",
                "properties": {
                    "schemas": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "Operations": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "op": {
                                    "type": "string",
                                    "enum": [
                                        "replace",
                                        "add",
                                        "remove"
                                    ]
                                },
                                "value": {
                                    "type": "object",
                                    "properties": {
                                        "active": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            },
                            "required": [
                                "op",
                                "value"
                            ]
                        }
                    }
                },
                "required": [
                    "Operations"
                ]
            },
            "ScimPartialUpdateGroupDto": {
                "type": "object",
                "properties": {
                    "schemas": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "Operations": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "op": {
                                    "type": "string",
                                    "enum": [
                                        "replace",
                                        "add",
                                        "remove"
                                    ]
                                },
                                "path": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "value": {
                                                "type": "string"
                                            },
                                            "display": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "value",
                                            "display"
                                        ]
                                    }
                                }
                            },
                            "required": [
                                "op",
                                "path",
                                "value"
                            ]
                        }
                    }
                },
                "required": [
                    "Operations"
                ]
            }
